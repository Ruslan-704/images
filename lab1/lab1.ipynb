   "source": [
    "import numpy as np\n",
    "\n",
    "# Создаем матрицу размером 10x10\n",
    "matrix = np.zeros((10, 10))\n",
    "\n",
    "# Устанавливаем единицы по краям матрицы\n",
    "matrix[0, :] = 1\n",
    "matrix[-1, :] = 1\n",
    "matrix[:, 0] = 1\n",
    "matrix[:, -1] = 1\n",
    "\n",
    "print(matrix)"
   ]
